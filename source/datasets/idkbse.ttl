    :isPartOf <common> ;
    :sourceData [ :uri "build/rdaterms.json.lines" ;
            :sourceData [ :uri "source/rda/content.ttl" ] ,
               [ :uri "source/rda/mediacarriers.ttl" ] ,
               [ :uri "source/categories/rdacategories.ttl" ] ,
               [ :uri "source/rda/labels.ttl" ] ] ;
               #[ :uri "source/rda/materials.ttl" ] ] ;
