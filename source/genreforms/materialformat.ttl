kbfmt:MoonScript a :TactileMaterial ;
  skos:exactMatch marc:Moon .

kbfmt:Braille a :TactileMaterial ;
  skos:exactMatch marc:TacMaterialType-b .

kbfmt:TactileCombination a :TactileMaterial ;
  skos:exactMatch marc:TacMaterialType-c .

kbfmt:TactileWithNoWritingSystem a :TactileMaterial ;
  skos:exactMatch marc:TactileWithNoWritingSystem .


kbfmt:ImageOnGlass a :ImageMaterial ;
  :broader kbfmt:Glass ;
  skos:exactMatch marc:NonProjectedType-d .

kbfmt:ImageOnStone a :ImageMaterial ;
  :broader kbfmt:Stone ;
  skos:exactMatch marc:NonProjectedType-s .

kbfmt:ImageOnWood a :ImageMaterial ;
