  skos:exactMatch marc:Betacam12InVideocassette . # TODO: smdb

kbfmt:BetacamSp12InVideocassette a :VideoFormat ;
  skos:exactMatch marc:BetacamSp12InVideocassette . # TODO: smdb

kbfmt:BluRayDisc a :VideoFormat ;
  skos:exactMatch marc:BluRayDisc , carrier:Blu-ray_Disc_\%28BD\%29 .

kbfmt:D234InVideocassette a :VideoFormat ;
  skos:exactMatch marc:D234InVideocassette . # TODO: smdb

