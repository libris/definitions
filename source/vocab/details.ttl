##
# Title information

:title a owl:DatatypeProperty;
    :category :shorthand;
    rdfs:label "title"@en, "titel"@sv;
    skos:definition "Ett namn som givits åt resursen."@sv;
    rdfs:subPropertyOf :label;
    owl:equivalentProperty dc:title;
    owl:propertyChainAxiom ( :hasTitle :mainTitle ) .

:hasTitle a owl:ObjectProperty;
    owl:equivalentProperty bf2:title;
    rdfs:range :Title;
    rdfs:label "har titel"@sv;
    rdfs:comment "Innehåller titelelement"@sv .

:Title a owl:Class;
    rdfs:subClassOf :StructuredValue;
    rdfs:label "Titel"@sv;
    owl:equivalentClass bf2:Title .

:mainTitle a owl:DatatypeProperty;
    rdfs:label "huvudtitel"@sv;
    rdfs:comment "Huvudsaklig titelkomponent"@sv;
    rdfs:subPropertyOf :value;
    owl:equivalentProperty bf2:mainTitle;
    rdfs:domain :Title .

:titleRemainder a owl:DatatypeProperty ;
    :category :pending ;
    rdfs:comment "Catch for unmatched 245$b properties at conversion from MARC21. Use ':subtitle' as preferred property."@en, "Egenskap som fångar upp omatchade 245$b vid konvertering från MARC21. Använd ':subtitle' som föredragen egenskap."@sv;
    rdfs:domain :Title ;
    rdfs:label "Övrig titelinformation"@sv .

:subtitle a owl:DatatypeProperty;
    rdfs:label "subtitle"@en, "övrig titelinformation"@sv;
    skos:altlabel "Undertitel"@sv;
    rdfs:comment "Ord eller tecken som bildar återstående del av titel efter huvudtitel."@sv;
    #rdfs:subPropertyOf :titleRemainder; - Commented out as not to depend on a pending property.
    owl:equivalentProperty bf2:subtitle;
    rdfs:domain :Title .

:partName a owl:DatatypeProperty;
    rdfs:label "part title"@en, "deltitel"@sv;
    rdfs:range rdfs:Literal;
    owl:equivalentProperty bf2:partName;
    sdo:domainIncludes :Title, :TitlePart .

:partNumber a owl:DatatypeProperty;
    rdfs:label "part number"@en, "delbeteckning"@sv;
    rdfs:range rdfs:Literal;
    owl:equivalentProperty bf2:partNumber;
    sdo:domainIncludes :Title, :TitlePart .

#:WorkTitle  a owl:Class;
#    owl:equivalentClass bf2:WorkTitle;
#    rdfs:subClassOf :Title;
#    rdfs:label "Work Title"@en, "Uniform titel"@sv .

:alternative a owl:DatatypeProperty;
    :category :shorthand;
    rdfs:label "alternative title"@en, "alternativ titel"@sv;
    owl:equivalentProperty dc:alternative .
    #TODO? owl:propertyChainAxiom ( :hasVariant :mainTitle ) .

:VariantTitle  a owl:Class;
    owl:equivalentClass bf2:VariantTitle;
    rdfs:subClassOf :Title;
    rdfs:label "Title variation"@en, "Varianttitel"@sv .

:AbbreviatedTitle a owl:Class;
    owl:equivalentClass bf2:AbbreviatedTitle;
    rdfs:subClassOf :VariantTitle;
    rdfs:label "Abbreviated title"@en, "Förkortad titel"@sv .

:abbreviatedTitle a owl:DatatypeProperty;
    :category :shorthand;
    rdfs:label "Abbreviated title"@en, "Förkortad titel"@sv;
    rdfs:subPropertyOf :title;
    rdfs:domain :Endeavour;
    owl:propertyChainAxiom (
                [ rdfs:subPropertyOf :hasTitle; rdfs:range :AbbreviatedTitle ]
                :mainTitle ) .

:KeyTitle a owl:Class ;
    owl:equivalentClass bf2:KeyTitle ;
    rdfs:subClassOf :VariantTitle;
    rdfs:label "Nyckeltitel"@sv .

:keyTitle a owl:DatatypeProperty;
    :category :shorthand;
    rdfs:label "Key title"@en, "Nyckeltitel"@sv;
    rdfs:subPropertyOf :title;
    owl:propertyChainAxiom (
                [ rdfs:subPropertyOf :hasTitle; rdfs:range :KeyTitle ]
                :mainTitle ) .

:ParallelTitle a owl:Class;
    owl:equivalentClass bf2:ParallelTitle;
    rdfs:subClassOf :VariantTitle;
    rdfs:label "Parallelltitel"@sv .

:CaptionTitle a owl:Class;
    rdfs:subClassOf :VariantTitle;
    rdfs:label "Caption title"@en, "Rubriktitel"@sv .

:CollectiveTitle a owl:Class;
    owl:equivalentClass bf2:CollectiveTitle;
    rdfs:subClassOf :VariantTitle;
    rdfs:label "Collective title"@en, "Samlingstitel"@sv .

:CoverTitle a owl:Class;
    rdfs:subClassOf :VariantTitle;
    rdfs:label "Cover title"@en, "Omslagstitel"@sv .

:DistinctiveTitle a owl:Class;
    :category :pending ;
    skos:changeNote "2020-04-17: Felaktigt använd i Libris, blockerad för tillägg via gränssnitt tillsvidare, använd Varianttitel istället."@sv;
    rdfs:subClassOf :VariantTitle;
    rdfs:label "Distinctive title"@en, "Titelvariant"@sv .

:FormerTitle a owl:Class;
    rdfs:subClassOf :VariantTitle;
    rdfs:label "Former title"@en, "Tidigare titel"@sv .

:RunningTitle a owl:Class;
    rdfs:subClassOf :VariantTitle;
    rdfs:label "Running title"@en, "Kolumntitel"@sv .

:SpineTitle a owl:Class;
    rdfs:label "Spine title"@en, "Ryggtitel"@sv;
    # rdfs:domain :Instance or propertyChainAxiom?
    rdfs:subClassOf :VariantTitle .

:AddedTitlePageTitle a owl:Class;
    rdfs:subClassOf :VariantTitle;
    rdfs:label "Added title page title"@en, "Förtitel"@sv .
    
:TitlePortion a owl:Class;
    rdfs:subClassOf :VariantTitle;
    rdfs:label "Title portion"@en, "Del av huvudtitel"@sv .
        
:TitlePart a owl:Class;
    rdfs:label "Title part"@en, "Titeldel"@sv .

:TranslatedTitle a owl:Class;
    rdfs:subClassOf :VariantTitle;
    rdfs:comment "Förekommer normalt endast i importerade poster"@sv;
    rdfs:comment "Översättning av titeluppgift som görs av katalogisatören när den översatta titeln inte finns som en parallelltitel på resursen."@sv, "Translation of the title proper that is made by the cataloging agency when the translated title does not appear as a parallel title on the item"@en;
    rdfs:label "Translated title"@en, "Översatt titel"@sv .


##
# Identifiers

:identifier a owl:DatatypeProperty ;
    :category :shorthand;
    rdfs:label "identifier"@en, "identifikator"@sv;
    rdfs:subPropertyOf :label;
    rdfs:range :Identifier;
    owl:equivalentProperty dc:identifier, bibo:identifier;
    owl:propertyChainAxiom ( :identifiedBy :value ) .

:isbn a owl:DatatypeProperty ;
    :category :shorthand;
    rdfs:label "ISBN";
    rdfs:subPropertyOf :identifier ; #TODO? relate to https://schema.org/isbn
    rdfs:range :ISBN .

:issn a owl:DatatypeProperty ;
    :category :shorthand;
    rdfs:label "ISSN";
    rdfs:subPropertyOf :identifier ; #TODO? relate to https://schema.org/issn
    rdfs:range :ISSN .

:doi a owl:DatatypeProperty ;
    :category :shorthand;
    rdfs:label "DOI";
    rdfs:subPropertyOf :identifier ;
    rdfs:range :DOI .

:ean a owl:DatatypeProperty ;
    :category :shorthand;
    rdfs:label "EAN";
    rdfs:subPropertyOf :identifier ;  #TODO? relate to https://schema.org/gtin
    rdfs:range :EAN .

:upc a owl:DatatypeProperty ;
    :category :shorthand;
    rdfs:label "UPC";
    rdfs:subPropertyOf :identifier ;  #TODO? relate to https://schema.org/gtin
    rdfs:range :UPC .

:uri a owl:DatatypeProperty;
    rdfs:domain :Resource;
    rdfs:subPropertyOf bibo:uri ;
    rdfs:range xsd:anyURI ;
    rdfs:comment "Sträng som unikt identifierar och anger sökvägen till en resurs."@sv;
    rdfs:label "URI"@en, "URI"@sv .

## Relations ->

:identifiedBy a owl:ObjectProperty ;
    rdfs:label "Identifier"@en, "Identifikator"@sv;
    skos:altLabel "Identifierad av"@sv;
    owl:equivalentProperty bf2:identifiedBy;
    owl:inverseOf :identifies;
    rdfs:comment "Teckensträng knuten till en resurs som används för att unikt särskilja resursen från andra."@sv;
    sdo:domainIncludes :Agent, :Document;
    rdfs:range :Identifier .

:indirectlyIdentifiedBy a owl:ObjectProperty ;
    rdfs:label "Indirectly identified by"@en, "Indirekt identifierad av"@sv;
    sdo:domainIncludes :Document;
    rdfs:range :Identifier ;
    rdfs:comment "En identifikator som indirekt är knuten till denna resurs (den kan vara hämtad från en resurs på annat format, eller vara en inkorrekt men ändå använd identifikator)."@sv;
    owl:propertyChainAxiom ( :hasEquivalent :identifiedBy ) .

:incorrectlyIdentifiedBy a owl:ObjectProperty ;
    rdfs:label "Incorrectly identified by"@en, "Inkorrekt identifierad av"@sv;
    sdo:domainIncludes :Document;
    rdfs:range :Identifier ;
    rdfs:comment "En identifikator som är inkorrekt använd för denna resurs. Identifikatorn i sig kan vara korrekt, invalid i formatet, eller indragen (vilket kan anges med status-egenskapen på själva identifikatorn). Denna relation används för att klargöra att den inte går att använda för att unikt identifiera resursen."@sv;
    skos:broadMatch :identifiedBy .

:identifies a owl:ObjectProperty;
    rdfs:label "Identifierad resurs"@sv; #english bflabel misspelled
    #rdfs:domain :Identifier;
    owl:inverseOf :identifiedBy;
    owl:equivalentProperty bf2:identifies .

