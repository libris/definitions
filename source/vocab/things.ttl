    :category :pending ;
    rdfs:label "Instans av rörlig bild"@sv;
    rdfs:subClassOf :Instance ;
    :inCollection marc:typeFromBib007;
    rdfs:subClassOf [ a owl:Restriction;
            owl:onProperty :instanceOf;
            owl:allValuesFrom :MovingImage
        ] .

