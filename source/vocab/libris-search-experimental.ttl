@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix : <https://id.kb.se/vocab/> .

@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

:LibrisQueryCode a rdfs:Datatype .

:author a owl:ObjectProperty ;
    :category :shorthand, :pending ;
    owl:equivalentProperty <https://id.kb.se/relator/author> ;
    skos:notation "FÖRF"^^:LibrisQueryCode ;
    owl:propertyChainAxiom (
            [ rdfs:subPropertyOf :contribution ; rdfs:range [ rdfs:subClassOf [ a owl:Restriction ;
                owl:onProperty :role ;
                owl:hasValue <https://id.kb.se/relator/author> ] ] ]
            :agent
        ) .

:isbn rdfs:domain :Instance . # See ./details.ttl for full definition of :isbn

##
# Platform terms
:and a owl:ObjectProperty ;
    :category :platform, :pending ;
    rdfs:label "and"@en, "och"@sv .

:or a owl:ObjectProperty ;
    :category :platform, :pending ;
    rdfs:label "or"@en, "eller"@sv .

:operator a owl:ObjectProperty ;
    :category :platform, :pending ;
    rdfs:label "operator"@en, "operator"@sv ;
    rdfs:range :Operator .

:Operator a owl:Class ;
    :category :platform, :pending ;
    rdfs:label "Operator"@en, "Operator"@sv .

:Equal a :Operator ;
    :category :platform, :pending ;
    rdfs:label "Equal to"@en, "Likamed"@sv ;
    skos:altLabel "=" .

:NotEqual a :Operator ;
    :category :platform, :pending ;
    rdfs:label "Not equal to"@en, "Ej likamed"@sv ;
    skos:altLabel "≠" .

:GreaterThanOrEqual a :Operator ;
    :category :platform, :pending ;
    rdfs:label "Greater than or equal to"@en, "Större än eller likamed"@sv ;
    skos:altLabel ">=" .

:GreaterThan a :Operator ;
    :category :platform, :pending ;
    rdfs:label "Greater than"@en, "Större än"@sv ;
    skos:altLabel ">" .

:LessThanOrEqual a :Operator ;
    :category :platform, :pending ;
    rdfs:label "Less than or equal to"@en, "Mindre än eller likamed"@sv ;
    skos:altLabel "<=" .

:LessThan a :Operator ;
    :category :platform, :pending ;
    rdfs:label "Less than"@en, "Mindre än"@sv ;
    skos:altLabel "<" .


