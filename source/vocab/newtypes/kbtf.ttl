  :prefLabel "Collecting Resource"@en ,
    "Samlande resurs"@sv .

t:MixedMaterial a :Class ;
  rdfs:subClassOf t:CollectingResource ;
  :prefLabel "Mixed Material"@en ,
    "Blandat material"@sv .

t:Kit a :Class ;
  rdfs:subClassOf t:CollectingResource ;
  :prefLabel "Kit"@en ,
    "Kit"@sv .

t:Collection a :Class ;
  rdfs:subClassOf t:CollectingResource ;
  :prefLabel "Collection"@en ,
    "Samling"@sv .

