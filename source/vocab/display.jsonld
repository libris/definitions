{
  "@context": {
    "fresnel": "http://www.w3.org/2004/09/fresnel#",
      "skos": "http://www.w3.org/2004/02/skos/core#",
      "owl": "http://www.w3.org/2002/07/owl#",
      "@base": "https://id.kb.se/vocab/display",
      "@vocab": "https://id.kb.se/vocab/",
      "marc": "https://id.kb.se/marc/",
      "lensGroups": {"@id": "@graph", "@container": "@index"},
      "lenses": {"@reverse": "fresnel:group", "@container": "@index"},
      "showProperties": {"@id": "fresnel:showProperties", "@container": "@list", "@type": "@vocab"},
      "classLensDomain": {"@reverse": "displayLens", "@type": "@vocab"},
      "inverseOf": {"@id": "owl:inverseOf", "@type": "@vocab"},
      "labelByLang": {"@id": "label", "@container": "@language"},
      "commentByLang": {"@id": "comment", "@container": "@language"},
      "prefLabelByLang": {"@id": "prefLabel", "@container": "@language"},
      "altLabelByLang": {"@id": "altLabel", "@container": "@language"},
      "noteByLang": {"@id": "note", "@container": "@language"},
      "titleByLang": {"@id": "title", "@container": "@language"},
      "descriptionByLang": {"@id": "description", "@container": "@language"}
  },
  "@graph": [
    {
      "@id": "https://id.kb.se/vocab/display.jsonld",
      "mainEntity": {"@id": "https://id.kb.se/vocab/display"}
    },
    {"@id": "https://id.kb.se/vocab/display"}
  ],
  "lensGroups": {
    "chips": {
      "@id": "chips",
      "@type": "fresnel:Group",
      "lenses": {
        "Resource": {
          "@type": "fresnel:Lens",
          "classLensDomain": "Resource",
          "showProperties": [ "title", "prefLabel", "label", "name", "notation", "code", "isPartOf", "isDefinedBy", "uri" ]
        },
        "Record": {
          "@type": "fresnel:Lens",
          "classLensDomain": "Record",
          "showProperties": [ "controlNumber", "inDataset" ]
        },
        "Instance": {
          "@type": "fresnel:Lens",
          "classLensDomain": "Instance",
          "showProperties": [ "hasTitle", "provisionActivity"]
        },
        "Work": {
          "@type": "fresnel:Lens",
          "classLensDomain": "Work",
          "showProperties": [ "hasTitle", "language" ]
        },
        "Item": {
          "@type": "fresnel:Lens",
          "classLensDomain": "Item",
          "showProperties": [ "itemOf", "heldBy" ]
        },
        "ShelfMark": {
          "@type": "fresnel:Lens",
          "classLensDomain": "ShelfMark",
          "showProperties": [ "label" ]
        },
        "Title": {
          "@type": "fresnel:Lens",
          "classLensDomain": "Title",
          "showProperties": [ "mainTitle", "title", "subtitle", "titleRemainder", "partNumber", "partName", "hasPart" ]
        },
        "TitlePart": {
          "@type": "fresnel:Lens",
          "classLensDomain": "Title",
          "showProperties": ["partNumber", "partName"]
        },
        "Identifier": {
          "@type": "fresnel:Lens",
          "classLensDomain": "Identifier",
          "showProperties": [ "value", "typeNote", "marc:hiddenValue" ]
        },
        "ProvisionActivity": {
          "@type": "fresnel:Lens",
          "classLensDomain": "ProvisionActivity",
          "showProperties": [ "place", "country", "agent", "year", "startYear", "endYear", "date", "marc:publicationStatus" ]
        },
        "Publication": {
          "@type": "fresnel:Lens",
          "classLensDomain": "Publication",
          "showProperties": [ "place", "agent", "year", "date" ]
        },
        "Serial": {
          "@type": "fresnel:Lens",
          "classLensDomain": "Serial",
          "showProperties": [ "seriesStatement", "seriesEnumeration" ]
        },
        "Country": {
          "@type": "fresnel:Lens",
          "classLensDomain": "Country",
          "showProperties": [ "prefLabel" ]
        },
        "Language": {
          "@type": "fresnel:Lens",
          "classLensDomain": "Language",
          "showProperties": [ "prefLabel" ]
        },
        "Person": {
          "@type": "fresnel:Lens",
          "classLensDomain": "Person",
          "showProperties": [ "familyName", "givenName", "name", "marc:numeration", "marc:titlesAndOtherWordsAssociatedWithAName", "marc:fullerFormName", "lifeSpan" ]
        },
        "Family": {
          "@type": "fresnel:Lens",
          "classLensDomain": "Family",
          "showProperties": [ "name", "marc:titlesAndOtherWordsAssociatedWithAName", "lifeSpan" ]
        },
        "Jurisdiction": {
          "@type": "fresnel:Lens",
          "classLensDomain": "Jurisdiction",
          "showProperties": [ "name", "isPartOf", "marc:subordinateUnit" ]
        },
        "Meeting": {
          "@type": "fresnel:Lens",
          "classLensDomain": "Meeting",
          "showProperties": [ "name", "marc:subordinateUnit", "date" ]
        },
        "Organization": {
          "@type": "fresnel:Lens",
          "classLensDomain": "Organization",
          "showProperties": [ "name", "isPartOf", "marc:subordinateUnit" ]
        },
        "Library": {
          "@type": "fresnel:Lens",
          "classLensDomain": "Library",
          "showProperties": [ "sigel" ]
        },
        "Concept": {
          "@type": "fresnel:Lens",
          "classLensDomain": "Concept",
          "showProperties": [ "prefLabel", "label", "notation", "inScheme", "inCollection" ]
        },
        "Classification": {
          "@type": "fresnel:Lens",
          "classLensDomain": "Classification",
          "showProperties": [ "code", "inScheme", "edition" ]
        },
        "Contribution": {
          "@type": "fresnel:Lens",
          "classLensDomain": "Contribution",
          "showProperties": [ "agent", "role" ]
        },
        "Relationship": {
          "@type": "fresnel:Lens",
          "classLensDomain": "Relationship",
          "showProperties": [ "entity", "relation" ]
        }
      }
    },
    "cards": {
      "@id": "cards",
      "@type": "fresnel:Group",
      "lenses": {
        "Resource": {
          "@type": "fresnel:Lens",
          "classLensDomain": "Resource",
          "showProperties": [ "title", "prefLabel", "label", "name", "notation", "code", "sameAs", "exactMatch", "closeMatch", "broadMatch", "isPartOf", "isDefinedBy" ]
        },
        "Record": {
          "@type": "fresnel:Lens",
          "classLensDomain": "Record",
          "showProperties": [ "controlNumber", "inDataset", "created", "modified", "descriptionCreator", "descriptionUpgrader", "descriptionLastModifier", "descriptionConventions", "identifiedBy" ]
        },
        "Instance": {
          "@type": "fresnel:Lens",
          "@id": "Instance-cards",
          "classLensDomain": "Instance",
          "showProperties": [
            "issuanceType",
            "mediaType",
            "carrierType",
            "hasTitle",
            "instanceOf",
            "responsibilityStatement",
            "editionStatement",
            "editionStatementRemainder",
            "publication",
            "marc:primaryProvisionActivity",
            "production",
            "manufacture",
            "distribution",
            "copyright",
            "copyrightDate",
            "identifiedBy",
            "indirectlyIdentifiedBy",
            "extent",
            "hasDimensions",
            "marc:otherPhysicalDetails",
            "accompaniedBy",
            "seriesMembership",
            {"inverseOf": "itemOf"}
          ]
        },
        "Work": {
          "@type": "fresnel:Lens",
          "classLensDomain": "Work",
          "showProperties": [ 
            "hasTitle",
            "musicMedium",
            "musicKey",
            "version",
            "contribution",
            "composition",
            "language",
            "genreForm",
            "classification",
            "subject" ]
        },
        "Item": {
          "@type": "fresnel:Lens",
          "classLensDomain": "Item",
          "showProperties": [ "itemOf", "heldBy", "hasComponent", "physicalLocation", "shelfMark" ]
        },
        "StructuredValue": {
          "@type": "fresnel:Lens",
          "classLensDomain": "StructuredValue",
          "showProperties": [ "label" ]
        },
        "ISBN": {
          "@type": "fresnel:Lens",
          "classLensDomain": "Identifier",
          "showProperties": [ "value", "qualifier" ]
        },
        "Library": {
          "@type": "fresnel:Lens",
          "classLensDomain": "Library",
          "showProperties": [ "sigel" ]
        },
        "Country": {
          "@type": "fresnel:Lens",
          "classLensDomain": "Country",
          "showProperties": [ "prefLabel", "notation", "isReplacedBy" ]
        },
        "DescriptionConventions": {
          "@type": "fresnel:Lens",
          "classLensDomain": "DescriptionConventions",
          "showProperties": [ "label", "code" ]
        },
        "Language": {
          "@type": "fresnel:Lens",
          "classLensDomain": "Language",
          "showProperties": [ "prefLabel", "label", "notation" ]
        },
        "MediaType": {
          "@type": "fresnel:Lens",
          "classLensDomain": "MediaType",
          "showProperties": [ "label", "code", "termGroup" ]
        },
        "CarrierType": {
          "@type": "fresnel:Lens",
          "classLensDomain": "CarrierType",
          "showProperties": [ "label", "code", "termGroup" ]
        },
        "ContentType": {
          "@type": "fresnel:Lens",
          "classLensDomain": "ContentType",
          "showProperties": [ "label", "code", "termGroup" ]
        },
        "Person": {
          "@type": "fresnel:Lens",
          "@id": "Person-cards",
          "classLensDomain": "Person",
          "showProperties": [ "familyName", "givenName", "name", "marc:numeration", "marc:titlesAndOtherWordsAssociatedWithAName", "marc:fullerFormName", "lifeSpan", "marc:attributionQualifier", "notation", "personTitle", "note", "identifiedBy", "hasVariant" ]
        },
        "Family": {
          "@type": "fresnel:Lens",
          "@id": "Family-cards",
          "classLensDomain": "Family",
          "showProperties": [ "name", "marc:titlesAndOtherWordsAssociatedWithAName", "lifeSpan", "hasTypeOfFamily", "note", "identifiedBy", "hasVariant" ]
        },
        "Jurisdiction": {
          "@type": "fresnel:Lens",
          "classLensDomain": "Jurisdiction",
          "showProperties": [ "name", "isPartOf", "marc:subordinateUnit", "note", "identifiedBy", "hasVariant" ]
        },
        "Meeting": {
          "@type": "fresnel:Lens",
          "classLensDomain": "Meeting",
          "showProperties": [ "name", "marc:subordinateUnit", "date", "note", "identifiedBy", "hasVariant" ]
        },
        "Organization": {
          "@type": "fresnel:Lens",
          "@id": "Organization-cards",
          "classLensDomain": "Organization",
          "showProperties": [ "name", "isPartOf", "marc:subordinateUnit", "note", "identifiedBy", "hasVariant" ]
        },
        "Concept": {
          "@type": "fresnel:Lens",
          "classLensDomain": "Concept",
          "showProperties": [ "prefLabel", "inScheme", "inCollection", "altLabel", "broader", "exactMatch", "closeMatch", "broadMatch",  {"inverseOf": "broader"}, "related", "scopeNote", "label", "notation", "note", "keyword" ]
        },
        "Contribution": {
          "@type": "fresnel:Lens",
          "classLensDomain": "Contribution",
          "showProperties": [ "agent", "role" ]
        },
        "SeriesMembership": {
          "@type": "fresnel:Lens",
          "classLensDomain": "SeriesMembership",
          "showProperties": [ "inSeries", "seriesStatement", "seriesEnumeration" ]
        },
        "Ontology": {
          "@type": "fresnel:Lens",
          "classLensDomain": "Ontology",
          "showProperties": [ "title", {"inverseOf": "isDefinedBy"} ]
        },
        "Cartographic": {
          "@type": "fresnel:Lens",
          "classLensDomain": "Cartographic",
          "showProperties": [ "marc:westernmostLongitudeCoordinates", "marc:easternmostLongitudeCoordinates", "marc:northernmostLatitudeCoordinates", "marc:southernmostLatitudeCoordinates" ]
        }
      }
    },
    "full": {
      "@id": "full",
      "@type": "fresnel:Group",
      "lenses": {
        "Instance": {
          "fresnel:extends": {"@id": "Instance-cards"},
          "showProperties": [ "instanceOf", "fresnel:super", {"inverseOf": "itemOf"} ]
        },
        "Family": {
          "fresnel:extends": {"@id": "Family-cards"},
          "showProperties": [ "fresnel:super", "hasTypeOfFamily", "establishDate", "terminateDate", "activityStartDate", "activityEndDate" ]
        },
        "Person": {
          "fresnel:extends": {"@id": "Person-cards"},
          "showProperties": [ "fresnel:super", "birthDate", "deathDate" ]
        },
        "Organization": {
          "fresnel:extends": {"@id": "Organization-cards"},
          "showProperties": [ "fresnel:super", "establishDate", "terminateDate", "activityStartDate", "activityEndDate" ]
        },
        "Item": {
          "fresnel:extends": {"@id": "Item-cards"},
          "showProperties": [ "fresnel:super", "shelfLabel", "shelfControlNumber" ]
        }
      }
    }
  }
}
